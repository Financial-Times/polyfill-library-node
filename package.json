{
  "name": "polyfill-library-node",
  "version": "1.0.0",
  "description": "To load polyfill-library polyfills into nodeJS",
  "main": "index.js",
  "engines": {
    "node": "^12"
  },
  "scripts": {
    "build": "node ./build.js",
    "commit": "git-cz",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "prepare": "npm run build",
    "postshrinkwrap": "npm run build",
    "test": "mocha tests.js"
  },
  "keywords": [
    "polyfill",
    "polyfill-library",
    "polyfill-library-node",
    "node polyfills"
  ],
  "author": "Jake Champion <me@jakechampion.name>",
  "license": "MIT",
  "dependencies": {
    "polyfill-library": "^3.42.0"
  },
  "devDependencies": {
    "hard-rejection": "^2.1.0",
    "mocha": "^7.0.1",
    "@commitlint/cli": "^8.1.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "commitizen": "^4.0.1",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.2",
    "proclaim": "^3.6.0",
    "supertest": "^4.0.2",
    "semantic-release": "^17.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md",
          "index.js"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]nn${nextRelease.notes}"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Financial-Times/polyfill-library-node.git"
  }
}
